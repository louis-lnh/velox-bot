datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model GuildConfig {
  id            String   @id @default(cuid())
  guildId       String   @unique
  autoroleId    String?
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
}
model Warning {
  id        String   @id @default(cuid())
  guildId   String
  userId    String
  moderator String
  reason    String
  createdAt DateTime @default(now())
}
